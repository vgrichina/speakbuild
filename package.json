{
  "name": "speakbuild",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "clean": "rm -rf node_modules/.cache && yarn cache clean",
    "evaluate-analysis": "bun scripts/evaluate-analysis.js",
    "evaluate-generation": "bun scripts/evaluate-generation.js",
    "build:ios": "eas build --platform ios --profile production",
    "build:android": "eas build --platform android --profile production",
    "submit:ios": "eas submit --platform ios --latest",
    "submit:android": "eas submit --platform android --latest"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-navigation/drawer": "^7.1.1",
    "dotenv": "^16.0.3",
    "expo": "~52.0.36",
    "expo-av": "~15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.0",
    "expo-file-system": "~18.0.7",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.4",
    "expo-linking": "~7.0.0",
    "expo-location": "~18.0.5",
    "expo-media-library": "~17.0.5",
    "expo-notifications": "~0.29.12",
    "expo-router": "~4.0.16",
    "expo-sensors": "~14.0.2",
    "expo-sharing": "~13.0.1",
    "expo-splash-screen": "~0.29.0",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.0",
    "partial-json": "^0.1.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-audio-record": "^0.2.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-mmkv": "^3.2.0",
    "react-native-permissions": "^5.2.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.11.1",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "typescript": "^5.3.3"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "lucide-react-native",
          "partial-json",
          "react-native-audio-record"
        ]
      }
    }
  }
}
