{
  "cli": {
    "version": ">= 5.9.1",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "distribution": "store",
      "prebuildCommand": "yarn inject-keys",
      "env": {
        "TEST_ULTRAVOX_KEY": "WILL_BE_PROVIDED_BY_CI",
        "TEST_OPENROUTER_KEY": "WILL_BE_PROVIDED_BY_CI"
      },
      "ios": {
        "resourceClass": "m-medium",
        "credentialsSource": "remote"
      },
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6743265036",
        "appleTeamId": "9532C74ZP2"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
